<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <local:StatusCallCollection x:Key="StatusCallCollectionKey"/>
        <local:SystemTypeCollection x:Key="SystemTypeCollectionKey"/>
        <local:JobCollection x:Key="JobCollectionKey"/>
        <local:DistanceTypeCollection x:Key="DistanceTypeCollectionKey"/>
        <local:ClosedCallFieldCollection x:Key="ClosedCallFieldCollectionKey"/>
        <local:ConvertUpdateToTrue x:Key="ConvertUpdateToTrueKey" />
        <local:NullToVisibilityConverter x:Key="NullToVisibilityConverterKey" />
        <local:TrueToVisibilityConverter x:Key="TrueToVisibilityConverterKey" />
        <local:OpenCallFieldCollection x:Key="OpenCallFieldCollectionKey" />
    </Application.Resources>
</Application>
